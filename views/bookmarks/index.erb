<ul>
  <% @bookmarks.each do |bookmark| %>
  <li>
    <a href=<%= bookmark.url %>>
      <%=bookmark.title%></a>
    </li>
  <% end %>
</ul>

<form action="/add" method='post'>
  <label for = "url"> url:</label>
  <input type="text" name="url"> <br>

  <label for = "title"> Title:</label>
  <input type="text" name="title"> <br>
  <input type="submit" value= "Add Bookmark">
</form>
